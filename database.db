CREATE DATABASE relief;

USE relief;
CREATE TABLE relief_login (
    login_id INT AUTO_INCREMENT PRIMARY KEY,
    Email VARCHAR(255),
    Username VARCHAR(255) UNIQUE,
    Password VARCHAR(255)
);
CREATE TABLE relief_users (
    USER_ID INT AUTO_INCREMENT PRIMARY KEY,
    Age INT NOT NULL,
    Weight INT NOT NULL,
    Height INT NOT NULL,
    Address VARCHAR(255) NOT NULL,
    Zipcode VARCHAR(255) NOT NULL,
    SSN VARCHAR(255) NOT NULL,
    login_id INT,
    verification INT,
    phone VARCHAR(255),
    name VARCHAR(255),
    eAddress VARCHAR(255),
    ePhone VARCHAR(255),
    eEmail VARCHAR(255),
    FOREIGN KEY (login_id) REFERENCES relief_login(login_id)
);

CREATE TABLE relief_answers (
    answer_id INT AUTO_INCREMENT PRIMARY KEY,
    q1 VARCHAR(1) NOT NULL,
    q2 VARCHAR(1) NOT NULL,
    q3 VARCHAR(1) NOT NULL,
    q4 VARCHAR(1) NOT NULL,
    q5 VARCHAR(1) NOT NULL,
    q6 VARCHAR(1) NOT NULL,
    q7 VARCHAR(1) NOT NULL,
    q8 VARCHAR(1) NOT NULL,
    q9 VARCHAR(1) NOT NULL,
    q10 VARCHAR(1) NOT NULL,
    user_healthcare VARCHAR(255),
    user_id INT,
    verification INT,
    FOREIGN KEY (user_id) REFERENCES relief_login(login_id)
);